{
  "name": "WeatherApp",
  "version": "1.0.0",
  "description": "Sample Weather App",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "coverage": "jest --coverage --collectCoverageFrom 'app/src/**'",
    "server": "node server.js",
    "dev": "webpack --progress -d --config config/dev.js --watch & node server.js",
    "prod": "webpack --progress -d --config config/prod.js --watch"
  },
  "keywords": [],
  "author": "Girdhar Malhotra",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.9",
    "enzyme": "^2.9.1",
    "eslint": "^4.17.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "jest": "^21.2.1",
    "lodash": "^4.17.5",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-3"
    ]
  },
  "jest": {
    "automock": false,
    "coverageReporters": [
      "lcov",
      "clover",
      "text",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/app/dist"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app/src"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^.*\\.scss$": "<rootDir>/app/src/testUtils/SCSSStub.js"
    },
    "testURL": "http://localhost:4000/"
  }
}
